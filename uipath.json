{
    "entryPoints": [
        {
            "filePath": "agent",
            "uniqueId": "28c5ddb9-ffa3-44e3-9a23-eb265d6f10c1",
            "type": "agent",
            "input": {
                "type": "object",
                "properties": {
                    "meeting_notes": {
                        "type": "string",
                        "description": "Raw meeting notes text to process"
                    },
                    "meeting_title": {
                        "type": "string",
                        "description": "Optional title of the meeting"
                    }
                },
                "required": ["meeting_notes"]
            },
            "output": {
                "type": "object",
                "properties": {
                    "meeting_summary": {
                        "type": "string",
                        "description": "Extracted meeting summary"
                    },
                    "extracted_issues": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "summary": {"type": "string"},
                                "description": {"type": "string"},
                                "issue_type": {"type": "string"},
                                "priority": {"type": "string"}
                            }
                        },
                        "description": "List of extracted issues from meeting"
                    },
                    "jira_tickets": {
                        "type": "array",
                        "items": {"type": "string"},
                        "description": "List of created JIRA ticket keys"
                    },
                    "confluence_pages": {
                        "type": "array",
                        "items": {"type": "string"},
                        "description": "List of created Confluence page URLs"
                    },
                    "status": {
                        "type": "string",
                        "description": "Processing status (Success/Failed/Error)"
                    },
                    "total_tickets": {
                        "type": "integer",
                        "description": "Total number of JIRA tickets created"
                    },
                    "total_pages": {
                        "type": "integer",
                        "description": "Total number of Confluence pages created"
                    }
                },
                "required": ["meeting_summary", "extracted_issues", "jira_tickets", "confluence_pages", "status", "total_tickets", "total_pages"]
            }
        }
    ],
    "bindings": {
        "version": "2.0",
        "resources": [],
        "environmentVariables": {
            "UIPATH_TELEMETRY_ENABLED": "true",
            "UIPATH_TRACE_LEVEL": "INFO",
            "AZURE_MONITOR_DISABLED": "true",
            "OPENTELEMETRY_SDK_DISABLED": "true",
            "OTEL_SDK_DISABLED": "true",
            "OTEL_TRACES_EXPORTER": "none",
            "OTEL_METRICS_EXPORTER": "none",
            "OTEL_LOGS_EXPORTER": "none",
            "AZURE_MONITOR_CONNECTION_STRING": "",
            "APPLICATIONINSIGHTS_CONNECTION_STRING": ""
        }
    }
